#include <stdio.h>
void even(int arr[],int n)
{
    for(int i=0;i<n;i++)
    {
        if(arr[i]%2 == 0)
        printf("%d",arr[i]);
    }
}
void odd(int arr[],int n)
{
    for(int i=0;i<n;i++)
    {
        if(arr[i]%2 != 0)
        printf("%d",arr[i]);
    }
}
int main()
{
    printf("Hello World");

    return 0;
}

