#include<stdio.h>
main()
{
    int a[10],n,i,se;
    printf("enter the number of variables to be used");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("enter the value of a[%d]",i);
        scanf("%d",&a[i]);
    }
    printf("enter the searching element ");
    scanf("%d",&se);
    for(i=0;i<n;i++)
    {
        if(a[i]==se)
        {
            printf("the element is found at position %d",i);
            break;
        }
    }
    return 0;
}
